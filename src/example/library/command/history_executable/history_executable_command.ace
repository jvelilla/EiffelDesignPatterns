system

	history_executable_command

root

	APPLICATION: make

default

	assertion (all)
	multithreaded (no)
	console_application (no)
	dynamic_runtime (no)
	dead_code_removal (yes)
	profile (no)
	line_generation (no)
	array_optimization (no)
	inlining (no)
	inlining_size ("10")
	debug (yes)

cluster

		-- Example classes
	history_executable_command: "${PATTERN}/src/example/library/command/history_executable"

		-- Command library
	command_library: "${PATTERN}/src/library/command/history_executable"

		-- Composite library
	composite_library: "${PATTERN}/src/library/composite/safety"

		-- EiffelBase library
	all base: "${ISE_EIFFEL}/library/base"
		exclude
			"table_eiffel3"; "desc"
		end

end
